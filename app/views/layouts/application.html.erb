<!DOCTYPE html>
<html>
<head>
  <title>DSC-SED | <%= content_for?(:title) ? yield(:title) : t('application.errors.missing_title') %></title>
  <%= render 'shared/stylesheets' %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= favicon_link_tag %>
</head>
<body>
  <div id="master">
    <%= render 'layouts/header' %>
    <%= render :partial => 'shared/flash', :locals => { :flash => flash } %>
    <%= yield %>
    <%= debug(params) if Rails.env.development? %>
  </div>
</body>
</html>
